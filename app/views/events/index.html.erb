<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<%=render "shared/session_handler"%>
<%= link_to "See all users", users_path%>


<h2>All Events</h2>
<%# <%=link_to "Create event",  new_event_path%>
<div class="calendar">
<%@events.sort_by(&:date).each do |event|%>
    <div class="event">
        <h3><%= event.name %></h3>
        <div><%= event.date %></div>
        <div><%= event.location %></div>
    </div>
<%end%>
</div>

